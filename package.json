{
  "name": "@carforyou/assets",
  "version": "0.0.0",
  "description": "Assets package to build and optimize our svgs",
  "scripts": {
    "version": "npm run build",
    "build": "pika build",
    "lint": "eslint --ext ts,js,tsx,jsx,json .",
    "format": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest",
    "test:integration": "sh __tests__/test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carforyou/carforyou-assets-pkg.git"
  },
  "author": "CAR FOR YOU",
  "license": "MIT",
  "homepage": "https://github.com/carforyou/assets-pkg#readme",
  "devDependencies": {
    "@carforyou/eslint-config": "3.1.10",
    "@pika/pack": "0.5.0",
    "@pika/plugin-build-node": "0.9.2",
    "@pika/plugin-simple-bin": "0.9.2",
    "@pika/plugin-ts-standard-pkg": "0.9.2",
    "@types/jest": "27.0.2",
    "jest": "27.0.1",
    "semantic-release": "17.0.3",
    "ts-jest": "27.0.5",
    "typescript": "4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "tsconfig": "tsconfig.pika.json"
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "cfy-assets"
        }
      ]
    ]
  },
  "dependencies": {
    "@types/node": "14.14.31",
    "svgo": "2.7.0",
    "@svgr/core": "5.5.0",
    "lodash.camelcase": "4.3.0",
    "lodash.upperfirst": "4.3.1",
    "create-index-normalized": "1.2.0"
  }
}
